<?xml version="1.0" encoding="UTF-8"?>
<web-app
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee"
    version="3.0">
   <servlet> 
      <servlet-name>login</servlet-name> 
      <servlet-class>com.indepfin.tallerdp.servlets.login</servlet-class> 
   </servlet>
   <error-page>
        <location>/general-error.html</location>
   </error-page>
   <servlet-mapping>
      <servlet-name>login</servlet-name> 
      <url-pattern>/login</url-pattern> 
   </servlet-mapping> 
   <servlet>
    	<servlet-name>GastosServlet</servlet-name>
    	<servlet-class>com.indepfin.tallerdp.servlets.GastosServlet</servlet-class>
    </servlet>
	<servlet-mapping>
    	<servlet-name>GastosServlet</servlet-name>
    	<url-pattern>/GastosServlet/*</url-pattern>
	</servlet-mapping>
</web-app>
